---
import { SITE } from 'astrowind:config';
import Layout from '~/layouts/PageLayout.astro';

import Features2 from '~/components/widgets/Features2.astro';

const meta = {
  title: SITE.title,
  description: SITE.description,
  dontUseTitleTemplate: true,
};
---

<Layout {meta}>
  <Features2
    title="Computer Science"
    subtitle="💡雑多なネタ帳"
    highlight=""
    items={[
      {
        title: 'LLM風説集',
        description:
          '2022/11末に公開されたChatGPTを中心としたLLMに関する非網羅的な論説集',
        icon: 'uil:science',
        url: 'https://github.com/coalg/diary/issues/12'
      },
      {
        title: '特異なバグの民間伝承',
        description:
          '常識とはかけ離れた問題でも、時には事実を映し出している',
        icon: 'mdi:bug',
        url: 'https://github.com/coalg/diary/issues/13'
      }
    ]}
  />

</Layout>
